                        <resource id="Magento_Config::config">
                            <resource id="HS_LoginAsCustomer::config_hs_login_as_customer" title="Hungersoft Login As Customer"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>