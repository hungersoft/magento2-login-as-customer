<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="HS_LoginAsCustomer::AdminLogin" sortOrder="10" title="Admin Login">
                    <resource id="HS_LoginAsCustomer::hs_login_as_customer_view" sortOrder="40" title="View Admin Login"/>
                </resource>
                <resource id="Magento_Backend::stores">
